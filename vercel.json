{
  "version": 2,
  "functions": {
    "api/dashboard-data.js": {
      "runtime": "@vercel/node"
    },
    "api/comparison-data.js": {
      "runtime": "@vercel/node"
    },
    "api/health.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
